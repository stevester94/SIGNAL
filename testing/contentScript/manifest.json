{
  "manifest_version": 2,

  "name": "Content Script Test",
  "description": "Content Script Test",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://www.reddit.com/*"
  ],
   "content_scripts": [
    {
      "matches": ["http://www.example.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}
